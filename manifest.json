{
  "manifest_version": 2,
  "name": "IntuitAnswer v1.0",
  "version": "1.0",
  "permissions": [
          "storage"
  ],
  "icons": {
    "128": "logo.png"
  },
  "content_scripts": [
    {
      "matches": [ "http://www.intuit.ru/studies/courses/*" ],
      "js": [ "jquery.js", "skript_find.js" ]
    }
  ],
  "browser_action": {
    "default_title": "IntuitAnswer",
    "default_icon": "logo.png",
    "default_popup": "popup.html"
  }
}